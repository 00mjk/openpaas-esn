.col-xs-12.conference-user-control-bar.text-center
  ul.list-inline
    li
      i.fa.fa-users.fa-2x.conference-toggle-invite-button(ng-click="toggleInviteRightBar()")
    li
      i.fa.fa-2x.conference-toggle-video-button(ng-click="toggleCamera()", ng-class="{'fa-eye': !videoMuted, 'fa-eye-slash': videoMuted}")
    li
      i.fa.fa-2x.conference-mute-button(ng-click="toggleSound()", ng-class="{'fa-microphone': !muted, 'fa-microphone-slash': muted}")
    li
      i.fa.fa-phone.fa-2x.conference-toggle-terminate-call-button(ng-click="leaveConference()")
