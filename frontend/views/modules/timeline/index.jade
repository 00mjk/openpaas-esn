sub-header
  timeline-subheader

.timeline.col-xs-9
  infinite-list(load-more-elements='loadNext()')
    ul.media-list
      li.media(ng-repeat="entry in timelineEntries")
        .t-view.m-b-0(data-tv-type="{{::entry.verb}}")
          ng-include(src="entry.templateUrl")

  ul.nav.nav-pills.nav-stacked(ng-if='!infiniteScrollCompleted')
    li.text-center(openpaas-logo-spinner, spinner-start-active='1', spinner-size='0.5')
