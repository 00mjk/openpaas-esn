.application-display.well.col-xs-12
  a.application-display-image-container(ng-href="/#applications/{{ application._id }}")
    img.application-display-image(src='/images/tux.png', alt='')
  .application-display-information
    h4.media-heading
      a(ng-href="/#applications/{{ application._id }}") {{ application.name }}
    p
      strong #{__('Redirect URI')}
      br
      span
        a(ng-href="{{application.redirectUri}}", target='_blank') {{ application.redirectUri }}
    p
      strong #{__('Client ID')}
      br
      span {{ application.clientId }}
    p
      strong #{__('Client Secret')}
      br
      span {{ application.clientSecret }}
