doctype html
html
  include ../commons/utils
  head
    meta(charset='utf8')
    base(href='/')
    title #{title} - OpenPaas
    meta(name="viewport", content="width=device-width, initial-scale=1")
    link(rel='icon', type='image/png', href='images/logo-tiny.png')
    link(rel='stylesheet', href='/components/animate.css/animate.css')
    link(rel='stylesheet', href='/components/roboto-fontface/css/roboto-fontface.css')
    link(rel='stylesheet', href='/css/modernizr.css')
    link(rel='stylesheet', href='/generated/css/oauth/styles.css')
    +injectCSSFromModules('oauth', injections)
    +injectJSFromModules('oauth', injections)
    +injectAngularFromModules('oauth', injections)
  body
    #wrap
      .vertiz-space
      include ./dialog
