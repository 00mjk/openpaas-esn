.collaboration-membership-requests-actions
  .btn.btn-icon.btn-link(href="" ng-click="ctrl.accept()" ng-disabled="ctrl.sending || ctrl.done", bs-tooltip, data-title=__('Accept'), data-placement="bottom")
    i.mdi.mdi-check

  .btn.btn-icon.btn-link(href="" ng-click="ctrl.decline()" ng-disabled="ctrl.sending || ctrl.done", bs-tooltip, data-title=__('Decline'), data-placement="bottom")
    i.mdi.mdi-close
