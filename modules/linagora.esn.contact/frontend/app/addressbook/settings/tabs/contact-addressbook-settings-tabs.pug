ul.esn-tab
  li.btn-wave(ng-class="{'active': $ctrl.selectedTab === 'main'}")
    a(href='', ng-click="$ctrl.selectedTab = 'main'")
      | #{__('Main')}
  li.btn-wave(ng-class="{'active': $ctrl.selectedTab === 'delegation'}")
    a(href='', ng-click="$ctrl.selectedTab = 'delegation'")
      | #{__('Delegation')}
