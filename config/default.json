{
  "webserver": {
    "enabled": true,
    "port": 8080,
    "virtualhosts": []
  },
  "wsserver": {
    "enabled": true,
    "port": 8080
  },
  "webrtc": {
    "enabled": true,
    "level": "debug"
  },
  "db": {
    "reconnectOnConfigurationChange": true
  },
  "log": {
    "file": {
      "enabled": true,
      "filename": "./log/application.log",
      "level": "info",
      "handleExceptions": true,
      "json": false,
      "prettyPrint": true,
      "colorize": false
    },
    "console": {
      "enabled": true,
      "level": "debug",
      "handleExceptions": true,
      "json": false,
      "prettyPrint": true,
      "colorize": true
    }
  },
  "auth": {
    "strategies": ["local", "mongo", "bearer"]
  }
}
